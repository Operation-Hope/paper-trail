# Copy this to .env and update with your local dev details. The .env *never* gets checked in.
# For local development, these defaults work if you have a local PostgreSQL database
# with the same credentials as Docker. If not, update the values below.

DB_HOST=localhost
DB_PORT=5432
DB_NAME=paper_trail_dev
DB_USER=paper_trail_user
DB_PASSWORD=dev_password_change_in_production
CONGRESS_GOV_API_KEY=
VERSION=dev
PORT=5001
FLASK_DEBUG=True
